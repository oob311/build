import 'package:flutter/material.dart';

void main() {
  int a,b;
  double pi = 3.14;
  if(pi<3){
    print("pi is less than 3");
  }else{
    print("pi is larger than 3");
  }
  var name = "batman";
  print("hello $name");
  int sum = add(5,9);
  print("sum is $sum");
  hello hh = new hello(name);
  hh.sayhello();
}

int add(int i, int j) {
  return i + j;
}

class hello{
  var name1;
  @override
  hello(name){
    this.name1 = name;
  }
  void sayhello(){
    print("hello $name1");
  }
}
